{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "vcs",
      "url": "https://gitlab.rlp.net/adwmainz/digicademy/dfd/dfd-typo3.git/"
    }
  ],
  "config": {
    "gitlab-domains": ["gitlab.rlp.net"],
    "process-timeout": 600,
    "preferred-install": "dist",
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true,
      "helhum/typo3-console-plugin": true
    }
  },
  "name": "digicademy/typo3-composer-install",
  "description": "TYPO3 11 LTS",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "foolz/sphinxql-query-builder": "^3.0",
    "georgringer/news": "^10.0",
    "helhum/typo3-console": "^7.1",
    "mehrwert/phpmyadmin": "^6.0",

    "typo3/cms-adminpanel": "^11.5",
    "typo3/cms-backend": "^11.5",
    "typo3/cms-belog": "^11.5",
    "typo3/cms-beuser": "^11.5",
    "typo3/cms-core": "^11.5",
    "typo3/cms-dashboard": "^11.5",
    "typo3/cms-extbase": "^11.5",
    "typo3/cms-extensionmanager": "^11.5",
    "typo3/cms-filelist": "^11.5",
    "typo3/cms-filemetadata": "^11.5",
    "typo3/cms-fluid": "^11.5",
    "typo3/cms-fluid-styled-content": "^11.5",
    "typo3/cms-form": "^11.5",
    "typo3/cms-frontend": "^11.5",
    "typo3/cms-impexp": "^11.5",
    "typo3/cms-info": "^11.5",
    "typo3/cms-install": "^11.5",
    "typo3/cms-linkvalidator": "^11.5",
    "typo3/cms-lowlevel": "^11.5",
    "typo3/cms-opendocs": "^11.5",
    "typo3/cms-recordlist": "^11.5",
    "typo3/cms-recycler": "^11.5",
    "typo3/cms-redirects": "^11.5",
    "typo3/cms-reports": "^11.5",
    "typo3/cms-rte-ckeditor": "^11.5",
    "typo3/cms-scheduler": "^11.5",
    "typo3/cms-seo": "^11.5",
    "typo3/cms-setup": "^11.5",
    "typo3/cms-sys-note": "^11.5",
    "typo3/cms-t3editor": "^11.5",
    "typo3/cms-tstemplate": "^11.5",
    "typo3/cms-viewpage": "^11.5",
    "typo3/cms-workspaces": "^11.5",

    "digicademy/dfd-typo3": "dev-main"
  },
  "autoload": {
    "psr-4": {
    },
    "files": [
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "htdocs"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": false
    }
  },
  "scripts":{
      "typo3-cms-scripts": [
          "typo3cms install:generatepackagestates"
      ],
      "post-autoload-dump": [
          "@typo3-cms-scripts"
      ]
  }
}
