{
	"name": "digicademy/typo3-composer-install",
	"description" : "Typo3 setup for a web app using the Cultural Heritage Framework",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"platform": {
			"php": "8.2"
		},
		"sort-packages": true
	},
	"repositories": [
		{
		"type": "vcs",
			"url": "https://github.com/digicademy-chf/chf_project.git"
		}
	],
	"require": {
		"digicademy/chf_project": "dev-main@dev"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
